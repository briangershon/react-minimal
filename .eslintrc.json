{
    "extends": ["airbnb-base", "plugin:react/recommended"],
    "env": {
      "browser": true,
      "jest/globals": true
    },
    "plugins": ["jest", "react"],
    "settings": {
      "react": {
        "createClass": "createReactClass",
        "pragma": "React",
        "version": "16.6.3"
      }
    },
    "rules": {
      "arrow-body-style": 0
    }
}
